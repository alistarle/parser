 . name
---------------
-> name [  key = "value" ]
 -> name [key="value"]
---------------
 --> name [  key = "value" ]
 --> name [key="value"]
---------------
 <- name [  key = "value" ]
 <- name [key="value"]
---------------
 <-- name [  key = "value" ]
 <-- name [key="value"]
---------------
s  <-- a <- f -> s --> d
 . s <-- a <- f -> s --> d
---------------
 <-- [  key = "value" ]
 <-- [key="value"]
---------------
 <-- [  key = "value" ] . [p=""]
 . [p=""] <-- [key="value"]
